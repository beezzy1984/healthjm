<?xml version="1.0" ?>
<data>
    <xpath expr="/form" position="replace_attributes" >
        <form string="Party" col="6" cursor="lastname" />
    </xpath>
    <xpath expr="/form/field[@name='lastname']" position="replace"/>
    <xpath expr="/form/label[@name='lastname']" position="replace" />
    <xpath expr="/form/notebook/page[@id=&quot;party_gnuhealth&quot;]/field[@name=&quot;insurance&quot;]" position="replace" />
    <xpath expr="/form//label[@name=&quot;alias&quot;]" position="replace" />

    <xpath expr="/form//field[@name='alias']" position="replace" />
    <xpath expr="/form//label[@name='unidentified']" position="replace" />
    <xpath expr="/form//field[@name='unidentified']" position="replace" />

    <xpath expr="/form/notebook/page[@id=&quot;party_gnuhealth&quot;]" position="after">
        <page string="Insurance" id="party_person_insurance" states="{'invisible': Not(Bool(Eval('is_person')))}">
            <field name="insurance" colspan="4" />
        </page>
    </xpath>

    <xpath expr="/form/notebook/page[@id=&quot;party_gnuhealth&quot;]/group[@id=&quot;person_details&quot;]/group" position="replace">
        <group id="person_detail_x" yfill="1" col="3">
            <group yfill="0" id="party_person_name_parts" string="Name">
                <label name="unidentified" />
                <field name="unidentified" />
                <label name="party_warning_ack" />
                <field name="party_warning_ack" />
                <newline />
                <label name="lastname"/>
                <field name="lastname"/>
                <label name="firstname"/>
                <field name="firstname"/>
                <label name="middlename"/>
                <field name="middlename"/>
                <label name="suffix"/>
                <field name="suffix"/>
                <label name="alias"/>
                <field name="alias"/>
                <label name="maiden_name" />
                <field name="maiden_name" />
                <newline /><separator colspan="4" id="sep1289" /><newline />
                <label name="mother_maiden_name" />
                <field name="mother_maiden_name" />
                <label name="father_name" />
                <field name="father_name" />
                
            </group>
            <newline />
            <group yfill="0" id="person_demographics" string="Demographics">
                <label name="sex"/>
                <field name="sex"/>
                <label name="dob"/>
                <field name="dob"/>
                <label name="marital_status"/>
                <field name="marital_status"/>
                <label name="du"/>
                <field name="du"/>
                <label name="education"/>
                <field name="education"/>
                <label name="occupation"/>
                <field name="occupation"/>
                <label name="citizenship"/>
                <field name="citizenship"/>
                <label name="residence"/>
                <field name="residence"/>
                <label name="ethnic_group"/>
                <field name="ethnic_group"/>
                <label name="alternative_identification"/>
                <field name="alternative_identification"/>
            </group>        
        </group>
    </xpath>

    <xpath expr="/form/notebook/page[@id=&quot;party_gnuhealth&quot;]/group[@id=&quot;person_details&quot;]/newline[last()]" position="after">
        <field colspan="4" name="alternative_ids" />
        <!-- ToDo: Figure out how to make this list taller. At least 3 rows -->
    </xpath>

    <xpath expr="/form/notebook/page[@id=&quot;general&quot;]" position="replace_attributes">
        <page string="Contact" id="general" />
    </xpath>

    <xpath expr="/form/field[@name=&quot;ref&quot;]" position="replace">
        <field name="upi" />
    </xpath>

</data>
