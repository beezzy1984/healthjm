<?xml version="1.0" encoding="utf-8" ?>
<tryton>
    <data>
        <!-- Health Centre Face Sheet -->
        <record model="ir.action.report" id="jmreport_facesheet_hc">
            <field name="name">Health Centre Facesheet</field>
            <field name="model">gnuhealth.patient</field>
            <field name="report_name">health_jamaica.report_facesheet_hc</field>
            <field name="active" eval="True" />
            <field name="template_extension">odt</field>
            <field name="extension"></field>
            <field name="report">health_jamaica/report/facesheet_hc.odt</field>
        </record>
        <record model="ir.action.keyword" id="jmkw_facesheet_hc"> 
            <field name="keyword">form_print</field> 
            <field name="model">gnuhealth.patient,-1</field> 
            <field name="action" ref="jmreport_facesheet_hc"/> 
        </record>
        <!-- Hospital Face Sheet -->
        <record model="ir.action.report" id="jmreport_facesheet_hosp">
            <field name="name">Hospital Facesheet</field>
            <field name="model">gnuhealth.patient</field>
            <field name="report_name">health_jamaica.report_facesheet_hosp</field>
            <field name="active" eval="True" />
            <field name="template_extension">odt</field>
            <field name="extension"></field>
            <field name="report">health_jamaica/report/facesheet_hosp.odt</field>
        </record>
        <record model="ir.action.keyword" id="jmkw_facesheet_hosp"> 
            <field name="keyword">form_print</field> 
            <field name="model">gnuhealth.patient,-1</field> 
            <field name="action" ref="jmreport_facesheet_hosp"/> 
        </record>

<!-- Patient Register Report -->
        <record model="ir.action.report" id="jmreport_patientregister">
            <field name="name">Patient Evaluation Register</field>
            <field name="model">gnuhealth.patient.evaluation</field>
            <field name="report_name">health_jamaica.report_patient_register</field>
            <field name="active" eval="True" />
            <field name="report">health_jamaica/report/patient_register.odt</field>
            <field name="template_extension">odt</field>
            <field name="extension"></field>
        </record>
        <record model="ir.ui.view" id="report_patientregister_start">
            <field name="model">healthjm.report.patientregister.start</field>
            <field name="type">form</field>
            <field name="name">report_patientregister_start</field>
        </record>
        <record model="ir.ui.view" id="report_patientregister_result">
            <field name="model">healthjm.report.patientregister.start</field>
            <field name="type">form</field>
            <field name="name">report_patientregister_result</field>
        </record>
        <record model="ir.action.wizard" id="action_patientregister_wizard">
            <field name="name">Patient Evaluation Register</field>
            <field name="wiz_name">healthjm.report.patientregister.wizard</field>
        </record>

