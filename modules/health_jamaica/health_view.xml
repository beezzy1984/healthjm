<?xml version="1.0"?>
<tryton>
  <data>

    <!-- Icons -->

    <record model="ir.ui.icon" id="gnuhealth_list_icon">
        <field name="name">gnuhealth-list</field>
        <field name="path">../health/icons/tryton-list.svg</field>
    </record>

    <!-- PartyPatient -->

    <record model="ir.ui.view" id="view_party_form">
       <field name="model">party.party</field>
       <field name="inherit" ref="party.party_view_form"/>
       <field name="name">party_form</field>
    </record>

    <record model="ir.ui.view" id="party_view_tree">
        <field name="model">party.party</field>
        <field name="inherit" ref="party.party_view_tree"/>
        <field name="name">party_tree</field>
    </record>    
        
    <record model="ir.ui.view" id="gnuhealth_alternative_identification_form">
       <field name="model">gnuhealth.person_alternative_identification</field>
       <field name="inherit" ref="health.gnuhealth_alternative_identification_form"/>
       <field name="name">alternativeID_form</field>
    </record>

    <record model="ir.ui.view" id="gnuhealth_alternative_identification_tree">
       <field name="model">gnuhealth.person_alternative_identification</field>
       <field name="inherit" ref="health.gnuhealth_alternative_identification_tree"/>
       <field name="name">alternativeID_tree</field>
    </record>

    <record model="ir.ui.view" id="gnuhealth_newborn_view">
       <field name="model">gnuhealth.newborn</field>
       <field name="type">form</field>
       <field name="name">newborn_form</field>
    </record>

    <record model="ir.ui.view" id="gnuhealth_du_view">
       <field name="model">gnuhealth.du</field>
       <field name="inherit" ref="health.gnuhealth_du_view"/>
       <field name="name">du_form</field>
    </record>
    <record model="ir.ui.view" id="gnuhealth_du_tree">
       <field name="model">gnuhealth.du</field>
       <field name="inherit" ref="health.gnuhealth_du_tree"/>
       <field name="name">du_tree</field>
    </record>

    <record model="ir.action.act_window" id="gnuhealth_action_post_office">
        <field name="name">Post Offices</field>
        <field name="res_model">country.post_office</field>
    </record>
    <menuitem action="gnuhealth_action_post_office"
        id="gnuhealth_menu_post_office" 
        parent="health.gnuhealth_demographics_menu"
        icon="gnuhealth-list" sequence="110"/>

    <record model="ir.ui.view" id="gnuhealth_district_community_tree_view">
       <field name="model">country.district_community</field>
       <field name="type">tree</field>
       <field name="name">district_community_tree</field>
    </record>


    <record model="ir.action.act_window" id="gnuhealth_action_district_community">
        <field name="name">District Communities</field>
        <field name="res_model">country.district_community</field>
    </record>
    <menuitem action="gnuhealth_action_district_community"
        id="gnuhealth_menu_district_community" 
        parent="health.gnuhealth_demographics_menu"
        icon="gnuhealth-list" sequence="120"/>

    <record model="ir.ui.view" id="healthjm_patient_tree">
        <field name="model">gnuhealth.patient</field>
        <field name="inherit" ref="health.gnuhealth_patient_tree"/>
        <field name="name">patient_tree</field>
    </record>
    <record model="ir.ui.view" id="healthjm_patient_form">
        <field name="model">gnuhealth.patient</field>
        <field name="inherit" ref="health.gnuhealth_patient_view_form"/>
        <field name="name">patient_form</field>
    </record>
    <record model="ir.ui.view" id="healthjm_address_view">
        <field name="model">party.address</field>
        <field name="inherit" ref="party.address_view_form"/>
        <field name="name">address_form_jm</field>
    </record>
    <record model="ir.ui.view" id="healthjm_patient_evaluation_view">
        <field name="model">gnuhealth.patient.evaluation</field>
        <field name="type">form</field>
        <field name="inherit" ref="health.gnuhealth_patient_evaluation_view" />
        <field name="name">patient_evaluation_form</field>
    </record>    
    <record model="ir.ui.view" id="healthjm_signs_symptoms_view">
        <field name="model">gnuhealth.signs_and_symptoms</field>
        <field name="inherit" ref="health.gnuhealth_signs_and_symptoms_form"/>
        <field name="name">signs_and_symptoms_form</field>
    </record>
    <record model="ir.ui.view" id="healthjm_signs_and_symptoms_tree">
        <field name="model">gnuhealth_signs_and_symptoms</field>
        <field name="inherit" ref="health.gnuhealth_signs_and_symptoms_tree"/>
        <field name="name">signs_and_symptoms_tree</field>
    </record>   
    <record model="ir.ui.view" id="healthjm_patient_procedure">
        <field name="model">gnuhealth.directions</field>
        <field name="inherit" ref="health.gnuhealth_directions_form"/>
        <field name="name">procedures_view_form</field>
    </record>
    <record model="ir.ui.view" id="healthjm_pathology_group_form">
        <field name="model">gnuhealth.pathology.group</field>
        <field name="type">form</field>
        <field name="inherit" ref="health.gnuhealth_pathology_group_form" />
        <field name="name">pathology_group_form</field>
    </record>
    <record model="ir.ui.view" id="healthjm_diagnostic_hypothesis_form">
        <field name="model">gnuhealth.diagnostic_hypothesis</field>
        <field name="type">form</field>
        <field name="inherit" ref="health.gnuhealth_diagnostic_hypothesis_form" />
        <field name="name">diagnostic_hypothesis_form</field>
    </record>
    <record model="ir.ui.view" id="healthjm_secondary_condition_form">
        <field name="model">gnuhealth.secondary_condition</field>
        <field name="type">form</field>
        <field name="inherit" ref="health.gnuhealth_secondary_condition_form" />
        <field name="name">secondary_condition_form</field>
    </record>    
    <record model="ir.action.act_window.domain" id="health.act_gnuhealth_appointment_domain_free">
        <field name="name">Scheduled</field>
        <field name="sequence" eval="10"/>
        <field name="domain">[('state', '=', 'free')]</field>
        <field name="act_window" ref="health.action_gnuhealth_appointment_view"/>
    </record>  -->   
  </data>
</tryton>
