<?xml version="1.0"?>
<form string="Remote Party">
	<group string="Party" col="10" id="party_ident_header">
		<field name="name" colspan="2"/>
		<label name="sex" />
		<field name="sex" width="40" />
		<label name="dob" />
		<field name="dob" width="40"/>
		<label name="upi" />
		<field name="upi" width="80"/>
	</group>
	<newline />
	<group col="10" id="party_ident_other">
		<label name="alias" colspan="1" />
		<field name="alias" colspan="1" />
		<label name="alt_ids" colspan="1" />
		<field name="alt_ids" colspan="3" />
		<newline/>
		<label name="marital_status" />
		<field name="marital_status" />
		<label name="maiden_name" />
		<field name="maiden_name" />
		<newline />
		<label name="father_name" />
		<field name="father_name" />
		<label name="mother_maiden_name" />
		<field name="mother_maiden_name" />
	</group>

	<newline />
	<button string="Import Party" name="fetch_remote_party" colspan="1" xalign="1.0" xexpand="0" />
</form>