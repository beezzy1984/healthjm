<?xml version="1.0"?>
<data>
    <xpath expr="//field[@name=&quot;neonatal_syndactyly&quot;]" position="after">
      <label name="bd"/>
      <field name="bd"/>
      <label name="bba"/>
      <field name="bba"/>
    </xpath>
    <xpath expr="//field[@name=&quot;mother&quot;]" position="replace">
    </xpath>
    <xpath expr="//group[@id=&quot;newborn_header&quot;]" position="replace">
    </xpath>
    <xpath expr="//group[@id=&quot;group_newborn_notes&quot;]" position="replace">
    </xpath>
    <xpath expr="//notebook" position="before">
        <group id="patient_header" string="Patient" colspan="4" col="8">
            <label name="patient" />
            <field name="patient" />
            <label name="birth_date" />
            <field name="birth_date" />
            <label name="sex" />
            <field name="sex" />
            <label name="patient_upi" />
            <field name="patient_upi" />
            <group id="mother_header" string="Mother's details" colspan="8" col="4">
                <label name="mother" />
                <field name="mother" />
                <label name="mother_upi" />
                <field name="mother_upi" />
            </group>
        </group>
    </xpath>
    <xpath expr="//group[@id=&quot;newborn_anthropometrics&quot;]" position="after">
        <group colspan="3" string="Notes" id="group_newborn_notes">
            <field colspan="3" name="notes"/>
        </group>
    </xpath>
</data>
