<?xml version="1.0" encoding="UTF-8"?>
<data>
    <xpath expr="/form" position="replace_attributes">
        <form string="Antenatal" />
    </xpath>
    <xpath expr="//label[@name=&quot;notes&quot;]" position="before">
        <group id="antenatal_fields" col="12" colspan="12">
            <group string="Signs" id="prenatal_evaluation_group_mother" colspan="12" col="8">
                <label name="hypertension"/>
                <field name="hypertension"/>
                <label name="preeclampsia"/>
                <field name="preeclampsia"/>
                <label name="overweight"/>
                <field name="overweight"/>
                <label name="diabetes"/>
                <field name="diabetes"/>
            </group>
            <group string="Screening" id="prenatal_evaluation_screening" colspan="12" col="6">
                <label name="screen_hb" />
                <field name="screen_hb" />
                <label name="screen_sickle" />
                <field name="screen_sickle" />
                <label name="tetanus" />
                <field name="tetanus" />
                <label name="screen_syphilis" />
                <field name="screen_syphilis" />
                <label name="screen_hiv" />
                <field name="screen_hiv" />
            </group>
            <group string="Placenta" id="prenatal_evaluation_group_placenta" colspan="12" col="6">
                <label name="placenta_previa"/>
                <field name="placenta_previa"/>
                <label name="invasive_placentation"/>
                <field name="invasive_placentation"/>
                <label name="vasa_previa"/>
                <field name="vasa_previa"/>
            </group>
            <newline/>
            <group string="Fetus Information" id="prenatal_evaluation_group_fetus" colspan="12" col="8">
                <label name="fundal_height"/>
                <field name="fundal_height"/>
                <label name="fetus_heart_rate"/>
                <field name="fetus_heart_rate"/>
                <label name="efw"/>
                <field name="efw"/>
                <newline/>
                <separator colspan="12" string="Fetal Biometry" id="separator_fetal_biometry"/>
                <newline/>
                <label name="fetal_bpd"/>
                <field name="fetal_bpd"/>
                <label name="fetal_hc"/>
                <field name="fetal_hc"/>
                <label name="fetal_ac"/>
                <field name="fetal_ac"/>
                <label name="fetal_fl"/>
                <field name="fetal_fl"/>
            </group>
        </group>
        <newline />
    </xpath>
    <xpath expr="//field[@name=&quot;notes&quot;]" position="after">
        <newline />
         <group id="grp_pregnancy" xexpand="1" >
            <label name="name" />
            <field name="name" xexpand="1" />
            <newline />
            <field name="evaluation_date" invisible="1" /><newline />
            <field name="healthprof" invisible="1" />
        </group>
    </xpath>
</data>
